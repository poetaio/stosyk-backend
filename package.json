{
  "name": "edu-platform",
  "version": "0.0.0",
  "description": "Educational Platform",
  "main": "index.js",
  "scripts": {
    "build-assets": "ts-node src/tools/copyAssets",
    "build": "tsc && build-assets",
    "start": "node build/index.js",
    "start-dev": "ts-node src/index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf dist/*",
    "dev": "tsc-watch --onCompilationStarted \"npm run build-assets\" --onSuccess \"npm run start-dev\"",
    "migrate": "npx sequelize-cli db:migrate",
    "db": "docker run -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=test -p 5433:5432 -d postgres"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/poetaio/EduPlatform.git"
  },
  "keywords": [
    "education"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/poetaio/EduPlatform/issues"
  },
  "homepage": "https://github.com/poetaio/EduPlatform#readme",
  "dependencies": {
    "apollo-server": "^3.7.0",
    "apollo-server-core": "^3.7.0",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.7.0",
    "aws-sdk": "^2.1124.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.7.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "redis": "^4.3.1",
    "redis-om": "^0.3.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.3",
    "sequelize-pg-utilities": "^2.0.1",
    "store2": "^2.13.2",
    "typeorm": "^0.3.10",
    "uuid": "^8.3.2",
    "winston": "^3.8.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^16.18.0",
    "eslint": "^8.12.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "14.16.1"
  }
}
